mer();\n        this.activeRandomMoe();\n      }\n    },\n    startMoe: function startMoe(moeId) {\n      this.isActive.splice(moeId, 1, true);\n    },\n    stopMoe: function stopMoe(moeId) {\n      this.isActive.splice(moeId, 1, false);\n    },\n    startTimer: function startTimer() {\n      var timer = setInterval(function () {\n        this.limitTime -= 1;\n\n        if (this.limitTime == 0) {\n          // end game\n          this.isPlaying = false; // update score\n\n          if (this.score > this.highscore) {\n            this.highscore = this.score;\n            this.score = 0;\n          }\n\n          clearInterval(timer);\n        }\n      }.bind(this), 1000);\n    },\n    activeRandomMoe: function activeRandomMoe() {\n      var timer = setInterval(function () {\n        var moeId = Math.floor(Math.random() * 4);\n        this.isActive[moeId] ? this.stopMoe(moeId) : this.startMoe(moeId);\n\n        if (!this.isPlaying) {\n          for (var i = 0; i < 4; i++) {\n            this.isActive.splice(i, 1, false);\n          }\n\n          clearInterval(timer);\n        }\n      }.bind(this), 500);\n    }\n  }\n};",{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;AAsBA,OAAA,IAAA,MAAA,QAAA;AACA,OAAA,OAAA,MAAA,WAAA;AACA,SAAA,WAAA,EAAA,aAAA,QAAA,QAAA;AACA,OAAA,GAAA,MAAA,KAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,KADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,IAAA,EAAA,IADA;AAEA,IAAA,OAAA,EAAA;AAFA,GAFA;AAMA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,SAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA,KAFA;AAGA,MAAA,UAAA,EAAA,IAHA;AAIA,MAAA,KAAA,EAAA,CAJA;AAKA,MAAA,SAAA,EAAA,CALA;AAMA,MAAA,QAAA,EAAA,CAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA;AANA,KAAA;AAQA,GAfA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA,EAAA,mBAAA,KAAA,EAAA;AACA,UAAA,KAAA,SAAA,EAAA;AACA,aAAA,KAAA,IAAA,CAAA;AACA,aAAA,OAAA,CAAA,KAAA;AACA;AACA,KANA;AAOA,IAAA,SAAA,EAAA,qBAAA;AACA,UAAA,CAAA,KAAA,SAAA,EAAA;AACA,aAAA,SAAA,GAAA,EAAA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,aAAA,UAAA;AACA,aAAA,eAAA;AACA;AACA,KAdA;AAeA,IAAA,QAfA,oBAeA,KA